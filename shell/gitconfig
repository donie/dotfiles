[init]
    defaultBranch = main
[core]
    pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true
    dark = true
    line-numbers = true
[merge]
    conflictStyle = zdiff3
[user]
	email = forinec@googlemail.com
	name = Li Sun
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
    clean = git-lfs clean -- %f
[url "git@github.com:"]
    insteadOf = https://github.com/
